{
  "name": "@sckrz/devbox",
  "version": "0.1.7",
  "description": "Developer Toolbox CLI - A comprehensive file and code management tool",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli.js"
    }
  },
  "bin": {
    "devbox": "./dist/cli.js",
    "devtoolbox": "./dist/cli.js",
    "dtb": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "start": "node dist/cli.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config jest.config.cjs",
    "test:playwright": "playwright test",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "prepublishOnly": "npm run build",
    "prepack": "npm run build"
  },
  "keywords": [
    "cli",
    "developer-tools",
    "file-management",
    "automation",
    "devtools",
    "toolbox",
    "utilities"
  ],
  "author": "raulshma",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/raulshma/devbox"
  },
  "homepage": "https://github.com/raulshma/devbox",
  "bugs": {
    "url": "https://github.com/raulshma/devbox/issues"
  },
  "dependencies": {
    "@inkjs/ui": "^2.0.0",
    "better-sqlite3": "^12.5.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "express": "^5.2.1",
    "ink": "^6.6.0",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "inquirer": "^9.3.8",
    "ora": "^9.0.0",
    "react": "^19.2.3",
    "trash": "^10.0.1",
    "uuid": "^13.0.0",
    "winston": "^3.19.0",
    "zod": "^4.3.5"
  },
  "optionalDependencies": {
    "keytar": "^7.9.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^5.0.6",
    "@types/inquirer": "^9.0.9",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.0",
    "@types/ora": "^3.1.0",
    "@types/react": "^19.2.7",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "playwright": "^1.40.1",
    "prettier": "^3.2.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
